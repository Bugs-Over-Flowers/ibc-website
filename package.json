    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db reset",
    "db:gen:types": "supabase gen types typescript --local > src/lib/supabase/db.types.ts",
    "test": "vitest",
    "test:unit": "vitest run __tests__/unit",
    "test:integration": "vitest run __tests__/integration",
    "test:component": "vitest run __tests__/component/vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:all": "vitest run && cypress run",
    "test:local": "bun run db:start && vitest",
    "test:local:ui": "bun run db:start && vitest --ui",
    "pretest": "bun run db:status || bun run db:start"
